/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "importHelpers": true,
        "target": "es2015",
        "module": "es2020",
        "skipLibCheck": true,
        // "noImplicitAny": true, # NOTE will be good to have this but there are ton of errors that needs to be addressed
        "lib": [
            "es2018",
            "dom"
        ]
    }
}
